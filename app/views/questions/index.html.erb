<h1>stack <strong>overflow</strong></h1>

<table>
  <thead>
    <tr>
      <th><h3>Top Questions</h3></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @questions.each do |question| %>
      <tr>
        <td><%= link_to "#{question.title}", question %></td>
        <td><%= question.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'New', new_question_path %>
